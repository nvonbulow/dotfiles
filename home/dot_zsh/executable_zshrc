# You probably want to edit files in ~/.zsh/configs

ANTIDOTE_INSTALL=~/.zsh/antidote
ANTIDOTE_HOME=~/.cache/antidote
source $ANTIDOTE_INSTALL/antidote.zsh

# Local config
[[ -f ~/.zshrc.local ]] && source ~/.zshrc.local

antidote load

source ~/.p10k.zsh
autoload -Uz promptinit && promptinit && prompt powerlevel10k

